function __removeSearchParam() {
  try {
    var url = new URL(window.location.href);
    if (url.searchParams.has("lmwr-ftv")) {
      url.searchParams.delete("lmwr-ftv");
      window.history.replaceState(null, "", url.href);
    } 
  } catch (e) {}
}
